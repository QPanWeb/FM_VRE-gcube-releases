<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui"
	xmlns:m="urn:import:org.gcube.portlets.user.tokengenerator.client.ui.subpanels">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel width="100%">
			<b:Accordion ui:field="accordionsPanel" width="100%">
				<b:AccordionGroup defaultOpen="true" heading="Personal Token" ui:field="contextToken">
				</b:AccordionGroup>
				<b:AccordionGroup heading="Qualified Token"
					ui:field="qualifiedTokens">
				</b:AccordionGroup>
				<b:AccordionGroup heading="Application Token"
					ui:field="applicationTokens">
				</b:AccordionGroup>
			</b:Accordion>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 