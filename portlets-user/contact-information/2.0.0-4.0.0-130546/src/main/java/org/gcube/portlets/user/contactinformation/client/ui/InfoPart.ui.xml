<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<g:HTMLPanel width="100%" styleName="contact-section"
		ui:field="panel">
		<table width="100%">
			<tr>
				<td width="5%">
					<g:Image height="auto" width="25px" ui:field="icon"></g:Image>
				</td>
				<td width="80%">
					<g:HTML ui:field="value" styleName="contact-section-value"></g:HTML>
					<b:TextBox ui:field="valueEdit" visible="false"
						styleName="contact-textbox"></b:TextBox>
				</td>
			</tr>
		</table>
	</g:HTMLPanel>
</ui:UiBinder> 