<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">

<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">

	<ui:style>
	.margin-none {
		margin-bottom: 0px;
	}
	
	.loading-image {
		color: red;
		background-image:
			url('data:image/gif;base64,');
	}
	</ui:style>

	<b:WellForm type="VERTICAL" ui:field="form_info" styleName='{style.margin-none}'>
		<b:Fieldset styleName="noBorder">

			<g:FlowPanel ui:field="loading_field"></g:FlowPanel>

			<b:ControlGroup ui:field="text_info_group">
				<b:ControlLabel for="input_text_info"></b:ControlLabel>
				<b:Controls>
					<b:TextArea placeholder="Empty" b:id="text_info"
						ui:field="text_info"></b:TextArea>
				</b:Controls>
			</b:ControlGroup>

			<g:HorizontalPanel ui:field="hp_form_actions"
				styleName="inputWmsDialog">
				<b:Button ui:field="close_dialog">Close</b:Button>
			</g:HorizontalPanel>
		</b:Fieldset>
	</b:WellForm>
</ui:UiBinder> 