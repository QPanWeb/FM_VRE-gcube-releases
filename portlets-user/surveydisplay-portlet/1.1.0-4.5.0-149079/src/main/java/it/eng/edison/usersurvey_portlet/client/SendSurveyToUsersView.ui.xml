<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel ui:field="verticalPanel">
			<b:WellForm ui:field="wellFormManage">
				<b:Heading ui:field="inviteSurveyHeading" size="4">Invite Users</b:Heading>
				<b:Heading ui:field="titleSurveyHeading" size="5" />
				<b:Label ui:field="toLabel">To: </b:Label> <b:Typeahead ui:field="inviteUsersToSurveyTypeahead"><b:TextBox ui:field="inviteUsersToSurveyTextBox"  placeholder="VRE Member name..." /></b:Typeahead><b:Button icon="PLUS_SIGN" ui:field="addUserToSendSurveyButton"
							addStyleNames="btn" />
				<b:Controls ui:field="sendSurveyControls">
					<b:Button icon="ARROW_LEFT" visible="true" ui:field="backToHomeSurveyButton">Back to Home</b:Button>
					<b:Button icon="ENVELOPE" visible="true" ui:field="sendSurveyButton">Send Survey to Users</b:Button>
				</b:Controls>
				<b:Controls>
				<br/>
				</b:Controls>
			</b:WellForm>
				<b:Controls>
					<b:Button icon="PLUS" visible="true" ui:field="addAllMembers">Add All VRE Members</b:Button>
					<b:Button icon="MINUS" visible="true" ui:field="removeAllMembers">Remove All VRE Members</b:Button>
				</b:Controls>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 