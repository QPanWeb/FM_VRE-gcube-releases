		name="functionalities" type="xs:int" minOccurs="0" maxOccurs="unbounded"/> 
		</xs:sequence> </xs:complexType> <xs:complexType name="SelectedFunctionality"> 
		<xs:sequence> <xs:element name="functionalityId" type="xs:int" minOccurs="1" 
		maxOccurs="1"/> <xs:element name="resourcesDescription" type="tns:SelectedResourceDescriptionType" 
		minOccurs="0" maxOccurs="unbounded"/> </xs:sequence> </xs:complexType> <xs:complexType 
		name="SelectedResourceDescriptionType"> <xs:sequence> <xs:element name="descriptionId" 
		type="xs:string"/> <xs:element name="resourceId" type="xs:string" minOccurs="0" 
		maxOccurs="unbounded"/> </xs:sequence> </xs:complexType> -->

	<!-- VRE Report Type -->
	<xs:complexType name="ReportList">
		<xs:sequence>
			<xs:element name="list" type="tns:Report" minOccurs="0"
				maxOccurs="unbounded" />
		</xs:sequence>
	</xs:complexType>

	<xs:complexType name="Report">
		<xs:sequence>
			<xs:element name="id" type="xs:string" />
			<xs:element name="name" type="xs:string" />
			<xs:element name="description" type="xs:string" />
			<xs:element name="state" type="xs:string" />
		</xs:sequence>
	</xs:complexType>
</xs:schema>