<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
          http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	version="3.0">

	<display-name>Email Comments Reader Webapp</display-name>

	<servlet>
		<servlet-name>start-email-reader</servlet-name>
		<servlet-class>org.gcube.portal.PortalSchedulerService</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>start-email-reader</servlet-name>
		<url-pattern>/start-email-reader</url-pattern>
	</servlet-mapping>



</web-app>
