								<Mimetype>text/xml</Mimetype>
								<Parameters>
									<Parameter isOptional="false" name="schema" value="-"/>
									<Parameter isOptional="false" name="language" value="-"/>
								</Parameters>
							</ContentType>
						</Target>
					</TransformationUnit>
				</TransformationUnits>
			</gDTSTransformationProgram>
		</Body>
	</Profile>
</Resource>

