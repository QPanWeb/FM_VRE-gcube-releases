<Resource>
	<ID>333333</ID>
	<Type>GenericResource</Type>
	<Profile>
		<Name>gDTSTP_DCtoTEIMetadataTransformer</Name>
		<Description>Composite program transforming metadata DC to TEI by using two existing TPs</Description>
		<Body>
			<gDTSTransformationProgram>
				<Transformer>
					<Class/>
					<GlobalProgramParameters/>
				</Transformer>
				<TransformationUnits>
					<TransformationUnit id="0" isComposite="true">
						<Sources>
							<Source>
								<Input id="TRInput0"/>
								<ContentType>
									<Mimetype>text/xml</Mimetype>
									<Parameters>
										<Parameter isOptional="false" name="schema" value="dc"/>
										<Parameter isOptional="false" name="language" value="*"/>
									</Parameters>
								</ContentType>
							</Source>
							<Source>
								<Input id="TRInput1"/>
								<ContentType>
									<Mimetype>text/xml</Mimetype>
									<Parameters>
										<Parameter isOptional="false" name="schema" value="lala"/>
										<Parameter isOptional="false" name="language" value="*"/>
									</Parameters>
								</ContentType>
							</Source>
						</Sources>
						<ProgramParameters/>
						<Target>
							<Output id="TROutput"/>
							<ContentType>
								<Mimetype>text/xml</Mimetype>
								<Parameters>
									<Parameter isOptional="false" name="schema" value="tei"/>
									<Parameter isOptional="false" name="language" value="*"/>
								</Parameters>
							</ContentType>
						</Target>
						<Composition>
							<ExtTransformationUnit tpid="genresource_1" tuid="0">
								<TargetInput targetID="TRInput0" thisID="TRInput0"/>
								<TargetOutput targetID="TROutput0" thisID="TRBridge1"/>
								<UnboundParameters/>
							</ExtTransformationUnit>
							<ExtTransformationUnit tpid="genresource_2" tuid="0">
								<TargetInput targetID="TRInput0" thisID="TRInput1"/>
								<TargetInput targetID="TRInput1" thisID="TRBridge1"/>
								<TargetOutput targetID="TROutput0" thisID="TROutput0"/>
								<UnboundParameters/>
							</ExtTransformationUnit>
						</Composition>
					</TransformationUnit>
				</TransformationUnits>
			</gDTSTransformationProgram>
		</Body>
	</Profile>
</Resource>

