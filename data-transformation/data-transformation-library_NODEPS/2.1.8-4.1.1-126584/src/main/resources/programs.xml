					<GlobalProgramParameters/>
				</Transformer>
				<TransformationUnits>
					<TransformationUnit id="0" isComposite="false">
						<Sources>
							<Source>
								<Input id="TRInput0"/>
								<ContentType>
									<Mimetype>text/xml</Mimetype>
									<Parameters>
										<Parameter isOptional="false" name="schema" value="-"/>
										<Parameter isOptional="false" name="language" value="-"/>
									</Parameters>
								</ContentType>
							</Source>
						</Sources>
						<ProgramParameters>
							<Parameter name="xsltid" value="-" isOptional="false"/>
						</ProgramParameters>
						<Target>
							<Output id="TROutput"/>
