<Resource>
    <ID/>
    <Type/>
    <Profile>
    	<SecondaryType>DTSTransformationProgram</SecondaryType>
        <Name>PDFToTextHTMLTransformer</Name>
        <Description>Program transforming pdf files to text or html</Description>
        <Body>
            <gDTSTransformationProgram>
                <Transformer>
                    <Class>org.gcube.datatransformation.datatransformationlibrary.programs.applications.PDFToTextHTMLTransformer</Class>
                    <GlobalProgramParameters/>
                </Transformer>
                <TransformationUnits>
                    <TransformationUnit id="0" isComposite="false">
                        <Sources>
                            <Source>
                                <Input id="TRInput0"/>
                                <ContentType>
                                    <Mimetype>application/pdf</Mimetype>
                                    <Parameters/>
                                </ContentType>
                            </Source>
                        </Sources>
                        <ProgramParameters/>
                        <Target>
                            <Output id="TROutput"/>
                            <ContentType>
                                <Mimetype>text/plain</Mimetype>
                                <Parameters/>
                            </ContentType>
                        </Target>
                    </TransformationUnit>
                    <TransformationUnit id="1" isComposite="false">
                        <Sources>
                            <Source>
                                <Input id="TRInput0"/>
                                <ContentType>
                                    <Mimetype>application/pdf</Mimetype>
                                    <Parameters/>
                                </ContentType>
                            </Source>
                        </Sources>
                        <ProgramParameters/>
                        <Target>
                            <Output id="TROutput"/>
                            <ContentType>
                                <Mimetype>text/html</Mimetype>
                                <Parameters/>
                            </ContentType>
                        </Target>
                    </TransformationUnit>
                </TransformationUnits>
            </gDTSTransformationProgram>
        </Body>
    </Profile>
</Resource>
