		<environment  name="reportingFrameworkRIContainerServiceClass" value="Execution"  type="java.lang.String" override="false" />
		<environment  name="reportingFrameworkRIContainerServiceName" value="WorkflowEngineService"  type="java.lang.String" override="false" />
		<environment  name="reportingFrameworkRIContainerServiceJNDIName" value="execution/workflowengine"  type="java.lang.String" override="false" />
		<environment  name="executionDirectoryCleanup" value="21600000"  type="java.lang.Long" override="false" />
		<environment  name="JMSProviderName" value="gr.uoa.di.madgik.environment.madgik.providers.JMSNotificationHandlingProvider"  type="java.lang.String" override="false" />
	</service>
</jndiConfig>
