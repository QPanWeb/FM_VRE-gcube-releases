<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='workflowtemplates'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	<!-- Other module inherits -->
	<inherits name='com.orange.links.Links' />
	<inherits name='com.google.gwt.widgetideas.WidgetIdeas' />
	<inherits name='com.google.gwt.libideas.LibIdeas' />
	<inherits name='com.extjs.gxt.ui.GXT' />
	<inherits name='com.allen_sauer.gwt.dnd.gwt-dnd' />
	<!-- inherits GCUBE Quick tour -->
	<inherits name='org.gcube.portlets.widgets.guidedtour.GuidedTour' />
	<inherits name='org.gcube.portlets.admin.wfdocslibrary.WfDocsLibrary' />
	<!-- inherits GCUBE Widgets -->
	<inherits name='org.gcube.portlets.user.gcubewidgets.WidgetFactory' />
	<!-- Specify the app entry point class. -->
	<entry-point
		class='org.gcube.portlets.admin.wftemplates.client.WorkflowTemplates' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />

</module>
