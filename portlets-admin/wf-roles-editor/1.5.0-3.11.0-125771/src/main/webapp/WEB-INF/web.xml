<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  
  <servlet>
    <servlet-name>WfRolesServiceServlet</servlet-name>
    <servlet-class>org.gcube.portlets.admin.wfroleseditor.server.WfRolesServiceImpl</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>WfRolesServiceServlet</servlet-name>
    <url-pattern>/wfroles/WfRolesService</url-pattern>
  </servlet-mapping>

  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>WfRoles.html</welcome-file>
  </welcome-file-list>

</web-app>
