<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	.noBorder {
		border: 0px;
	}
	</ui:style>
	<g:HTMLPanel>
		<g:HTMLPanel ui:field="form_unit_fields">
			<b:Form type="HORIZONTAL">
				<b:Fieldset styleName="{style.noBorder}">
					<b:ControlGroup ui:field="cg_questionnaire_id">
						<b:ControlLabel for="cl_questionnaire_id">New Questionnaire Id</b:ControlLabel>
						<b:Controls>
							<b:TextBox placeholder="Type the Questionnaire Id...."
								title="This is the Questionnaire id" b:id="field_questionnaire_id"
								ui:field="field_questionnaire_id"></b:TextBox>
						</b:Controls>
					</b:ControlGroup>
				</b:Fieldset>
			</b:Form>
		</g:HTMLPanel>
		<b:Pager left="Older" right="Associate Questionnaire" aligned="true"
			ui:field="pager" />
		<g:HTMLPanel ui:field="quest_footer_panel">
		</g:HTMLPanel>
	</g:HTMLPanel>
</ui:UiBinder> 