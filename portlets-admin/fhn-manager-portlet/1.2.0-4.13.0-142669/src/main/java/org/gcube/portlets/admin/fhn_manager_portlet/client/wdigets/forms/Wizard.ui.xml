<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:b="urn:import:com.github.gwtbootstrap.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel>
	<g:DialogBox title="" ui:field="dialog" modal="true">
		<g:caption ui:field="caption"> suca</g:caption>
		
		<g:DockLayoutPanel ui:field="dock" height="400px" width="600px">
			<g:center>
			<g:SimpleLayoutPanel ui:field="centerContainer"></g:SimpleLayoutPanel>
			</g:center>
			<g:south size="50">
				<b:Navbar>
				<b:Nav><g:FocusPanel ui:field="back" visible="false">
				<g:HTMLPanel><a class="btn"><i class="fa fa-backward"></i>Back</a></g:HTMLPanel></g:FocusPanel>
				</b:Nav>
				<b:Nav><g:FocusPanel ui:field="createNew" visible="false"><g:HTMLPanel>
				<a class="btn btn-primary"><i class="fa fa-magic"></i>Create New</a></g:HTMLPanel></g:FocusPanel>
				</b:Nav>
				<b:Nav><g:FocusPanel ui:field="next"><g:HTMLPanel><a class="btn">
				Next<i class="fa fa-forward"></i></a></g:HTMLPanel></g:FocusPanel>
				</b:Nav>
				<b:Nav><g:FocusPanel ui:field="close"><g:HTMLPanel><a class="btn">
				<i class="fa fa-times"></i>Close</a></g:HTMLPanel></g:FocusPanel>
				</b:Nav>
				</b:Navbar>
			</g:south>
		</g:DockLayoutPanel>
	</g:DialogBox>
	</g:HTMLPanel>
</ui:UiBinder> 